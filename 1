#include<iostream>
#include<vector>
using namespace std;
int main()
{ 
    int n=0,max=0,min=0,sum=0;
 int minsum=0,maxsum=0;
cin>>n;
   vector<int>a(5); 
   for(int i=0;i<5;i++)
    {
        cin>>a[i];
    }
 
    
    for(int i=0;i<5;i++)
    {
        sum+=a[i];
    }
      for(int i=0;i<5;i++)
{
    if(max<a[i])
    {
        max=a[i];
    }
    if(min>a[i])
    {
        min=a[i];
    }
    }
    minsum=sum-max;
    maxsum=sum-min;
    cout<<minsum<<" "<<maxsum;
    
    return 0;
}
