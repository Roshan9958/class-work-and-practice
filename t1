#include <iostream>
#include<iomanip>
using namespace std;



int main()
{

    string s;
    cin>>s;
    
    if(s[8]=='P' && s[0]!='1' && s[1]!='2')
    {
        int a=s[0]-'0';
        int b=s[1]-'0';
        a+=1;
        b+=2;
        if(b>=10)
        { b=b%10;
            a++;
        }
        s[0]=a+'0';
        s[1]=b+'0';
    }
    if(s[8]=='A'&&s[0]=='1'&&s[1]=='2')
    {
        s[0]='0';
        s[1]='0';
    }
    
    for(int i=0;i<8;i++)
    {
        cout<<s[i];
    }
    return 0;
}
